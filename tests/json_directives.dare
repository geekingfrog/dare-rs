#[json(tag="myTag", content="myContent")]
enum EnumObject {
  // example of an alias
  Simple1 as "one",
  Other(List<Int8>),
  SimpleThree(Bytes, String, List<Bool>),
}

#[json(repr="tuple")]
enum EnumTuple {
  TupleSimple1 as "one",
  TupleOther(Bytes),
  TupleThree(Bytes, List<Int>, String) as "tuple_three",
}

#[json(repr="union")]
enum EnumUnion {
  UnionSimple1 as "one",
  UnionSimple2,
  UnionOther(Bytes),
  UnionString(String),
  UnionThree(Bytes, List<Int>, String) as "tuple_three",
}
